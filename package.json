{
  "name": "@dsnp/parquetjs",
  "description": "fully asynchronous, pure JavaScript implementation of the Parquet file format",
  "main": "dist/parquet.js",
  "version": "0.0.0",
  "homepage": "https://github.com/LibertyDSNP/parquetjs",
  "license": "MIT",
  "keywords": [
    "dremel",
    "parquet"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/LibertyDSNP/parquetjs.git"
  },
  "dependencies": {
    "@types/bson": "^4.0.3",
    "@types/long": "^4.0.1",
    "@types/node": "^14.14.35",
    "@types/thrift": "^0.10.10",
    "assert": "^2.0.0",
    "brotli": "^1.3.0",
    "browserify-zlib": "^0.2.0",
    "bson": "4.4.0",
    "buffer": "^6.0.3",
    "cross-fetch": "^3.1.4",
    "events": "^3.3.0",
    "int53": "^0.2.4",
    "lodash": "^4.17.21",
    "long": "^4.0.0",
    "lzo": "^0.4.0",
    "object-stream": "0.0.1",
    "path-browserify": "^1.0.1",
    "readable-stream": "^3.6.0",
    "snappyjs": "^0.6.0",
    "thrift": "0.14.1",
    "util": "^0.12.4",
    "varint": "^5.0.0",
    "wasm-brotli": "^2.0.2",
    "xxhash-wasm": "^0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^10.0.0",
    "@webpack-cli/generators": "^2.2.0",
    "browserfs": "^1.4.3",
    "buffer-browserify": "^0.2.5",
    "chai": "4.3.4",
    "mocha": "8.3.2",
    "msw": "^0.29.0",
    "process": "^0.11.10",
    "sinon": "^10.0.0",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "browser": {
    "thrift": "./node_modules/thrift/lib/nodejs/lib/thrift/browser.js",
    "util": "util",
    "stream": "readable-stream",
    "assert": "assert",
    "path": "path-browserify",
    "events": "events",
    "zlib": "browserify-zlib",
    "fs": "browserfs"
  },
  "scripts": {
    "build": "npm run build:node",
    "build:node": "tsc -b",
    "build:browser": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "type": "tsc --noEmit",
    "lint": "echo 'Linting, it is on the TODO list...'",
    "test": "mocha -r ts-node/register 'test/**/*.{js,ts}'",
    "clean": "rm -Rf ./dist",
    "prepublishOnly": "npm run clean && npm run build",
    "thrift": "thrift -out gen-nodejs --gen js:ts parquet.thrift && thrift -out gen-nodejs --gen js:node parquet.thrift",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "files": [
    "dist/**/*",
    "parquet.thrift",
    "*.md",
    "docs/**/*"
  ]
}
